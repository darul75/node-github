  {
  "defines": {
    "constants": {
      "name": "Linked",
      "description": "A Node.JS module, which provides an object oriented wrapper for the Linked API.",
      "protocol": "https",
      "host": "api.linkedin.com",
      "port": 443,
      "dateFormat": "YYYY-MM-DDTHH:MM:SSZ",
      "requestFormat": "json",
      "requestMedia": "application/vnd.github.v3+json"
    },
    "response-headers": [
      "X-RateLimit-Limit",
      "X-RateLimit-Remaining",
      "X-RateLimit-Reset",
      "X-Oauth-Scopes",
      "Link",
      "Location",
      "Last-Modified",
      "Etag",
      "Status"
    ],
    "request-headers": [
      "If-Modified-Since",
      "If-None-Match",
      "Cookie",
      "User-Agent",
      "Accept",
      "X-Linked-OTP"
    ],
    "profile-request-headers": [
      "Accept-Language HTTP header"
    ],
    "params": {
      "files": {
        "type": "Json",
        "required": true,
        "validation": "",
        "invalidmsg": "",
        "description": "Files that make up this gist. The key of which should be a required string filename and the value another required hash with parameters: 'content'"
      },
      "url-field-selector": {
        "type": "String",
        "required": false,
        "validation": "",
        "invalidmsg": "",
        "description": "Explicitly request the data you want, more details here : https://developer.linkedin.com/documents/understanding-field-selectors"
      },
      "id": {
        "type": "String",
        "required": false,
        "validation": "",
        "invalidmsg": "",
        "description": ""
      },
      "idOrUrl": {
        "type": "String",
        "required": true,
        "validation": "",
        "invalidmsg": "",
        "description": "Profile Id or public URL"
      },      
      "group-id": {
        "type": "String",
        "required": false,
        "validation": "",
        "invalidmsg": "",
        "description": ""
      },
      "person-id": {
        "type": "String",
        "required": true,
        "validation": "",
        "invalidmsg": "",
        "description": ""
      },
      "company-id": {
        "type": "String",
        "required": true,
        "validation": "",
        "invalidmsg": "",
        "description": ""
      },
      "universal-name": {
        "type": "String",
        "required": true,
        "validation": "",
        "invalidmsg": "",
        "description": ""
      },
      "email-domain": {
        "type": "String",
        "required": true,
        "validation": "",
        "invalidmsg": "",
        "description": ""
      },
      "url": {
        "type": "String",
        "required": false,
        "validation": "",
        "invalidmsg": "",
        "description": "Public profile URL"
      },
      "secure-urls": {
        "type": "Boolean",
        "required": false,
        "validation": "",
        "invalidmsg": "",
        "description": "Indicate that you want the URLs in your response to be HTTPS"
      },
      "start": {
        "type": "Number",
        "required": false,
        "validation": "^[0-9]+$",
        "invalidmsg": "",
        "description": ""
      },
      "end": {
        "type": "Number",
        "required": false,
        "validation": "^[0-9]+$",
        "invalidmsg": "",
        "description": ""
      },
      "count": {
        "type": "Number",
        "required": false,
        "validation": "^[0-9]+$",
        "invalidmsg": "",
        "description": ""
      },
      "modified": {
        "type": "String",
        "required": false,
        "validation": "",
        "invalidmsg": "",
        "description": "Values are: 'updated' or 'new'"
      },
      "modified-since": {
        "type": "Number",
        "required": false,
        "validation": "",
        "invalidmsg": "",
        "description": "In miliseconds since epoch"
      },
      "membership-state": {
        "type": "Array",
        "required": false,
        "validation": "",
        "invalidmsg": "",
        "description": ""
      },
      "order": {
        "type": "Array",
        "required": false,
        "validation": "",
        "invalidmsg": "",
        "description": ""
      },
      "role": {
        "type": "Array",
        "required": false,
        "validation": "",
        "invalidmsg": "",
        "description": ""
      },
      "category": {
        "type": "Array",
        "required": false,
        "validation": "",
        "invalidmsg": "",
        "description": ""
      },
      "is-company-admin": {
        "type": "Boolean",
        "required": false,
        "validation": "",
        "invalidmsg": "",
        "description": ""
      },
      "event-type": {
        "type": "String",
        "required": false,
        "validation": "",
        "invalidmsg": "",
        "description": ""
      },
      "company-update-key": {
        "type": "String",
        "required": false,
        "validation": "",
        "invalidmsg": "",
        "description": ""
      }
    }
  },
  "people": {
    "get-current": {            
      "url": "/people/~",
      "doc-url": "https://developer.linkedin.com/documents/profile-api",
      "method": "GET",
      "params": {
        "$url-field-selector": null,
        "$secure-urls": null
      }
    },
    "get-member": {
      "url": "/people/",
      "doc-url": "https://developer.linkedin.com/documents/profile-api",
      "method": "GET",
      "params": {
        "$id": null,
        "$url": null,
        "$url-field-selector": null,
        "$secure-urls": null
      }
    },    
    "get-current-connections": {
      "url": "/people/~/connections",
      "doc-url": "https://developer.linkedin.com/documents/connections-api",
      "method": "GET",
      "params": {
        "$start": null,
        "$count": null,
        "$modified": null,
        "$modified-since":null,
        "$url-field-selector": null
      }
    },
    "get-member-connections": {
      "url": "/people/:idOrUrl/connections",
      "doc-url": "https://developer.linkedin.com/documents/connections-api",
      "method": "GET",
      "params": {
        "$idOrUrl": null,
        "$start": null,
        "$count": null,
        "$modified": null,
        "$modified-since":null
      }
    },
    "search": {
      "url": "/people-search",
      "doc-url": "https://developer.linkedin.com/documents/people-search-api",
      "method": "GET",
      "params": {
        "$keywords": null,
        "$first-name": null,
        "$last-name": null,
        "$school-name": null,
        "$start": null,
        "$count": null,
        "$sort":null
      }
    }    
  },
  "groups": {
    "get-details": {
      "url": "/groups/:group-id",
      "doc-url": "https://developer.linkedin.com/documents/groups-api#profiledetails",
      "method": "GET",
