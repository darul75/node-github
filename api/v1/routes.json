    }, 
   "get-posts": {
      "url": "/groups/:group-id/posts",
      "doc-url": "https://developer.linkedin.com/documents/groups-api#groupposts",
      "method": "GET",
      "params": {
        "$group-id": null,
        "$start": null,
        "$count": null,
        "$role": null,
        "$category": null,
        "$url-field-selector": null
      }
    },
   "get-memberships-posts": {
      "url": "/people/~/group-memberships/:group-id/posts",
      "doc-url": "https://developer.linkedin.com/documents/groups-api#groupposts",
      "method": "GET",
      "params": {
        "$group-id": null,
        "$start": null,
        "$count": null,
        "$role": null,
        "$category": null,
        "$url-field-selector": null
      }
    },
    "get-suggestions-posts": {
      "url": "/people/~/suggestions/groups/:group-id/posts",
      "doc-url": "https://developer.linkedin.com/documents/groups-api#groupposts",
      "method": "GET",
      "params": {
        "$group-id": null,
        "$start": null,
        "$count": null,
        "$role": null,
        "$category": null,
        "$url-field-selector": null
      }
    },
   "add-post": {
      "url": "/people/~/groups/:group-id/posts",
      "doc-url": "https://developer.linkedin.com/documents/groups-api#create",
      "method": "POST",
      "params": {}
    },    
    "get-post": {
      "url": "/posts/:post-id",
      "doc-url": "https://developer.linkedin.com/documents/groups-api#post",
      "method": "GET",
      "params": {        
        "$post-id": null,           
        "$url-field-selector": null
      }
    },
    "get-posts-comments": {
      "url": "/posts/:post-id/comments",
      "doc-url": "https://developer.linkedin.com/documents/groups-api#post",
      "method": "GET",
      "params": {
        "$group-id": null,
        "$post-id": null,
