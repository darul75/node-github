  "company": {
    "all": {
      "url": "/companies",
      "method": "GET",
      "params": {
        "$url-field-selector": null,
        "$is-company-admin": null,
        "$start": null,
        "$count": null
      }
    },
    "one": {
        "url": "/companies/:company-id",
        "method": "GET",
        "params": {
          "$company-id": null
        }
    },
    "one-by-universal-name": {
        "url": "/companies/",
        "method": "GET",
        "params": {
          "$universal-name": null
        }
    },
    "one-by-email-domain": {
